{
	"pages": [ //pages数组中第一项表示应用启动页，参考：https://uniapp.dcloud.io/collocation/pages
		{
			"path": "pages/index/index",
			"style": {
				"navigationBarTitleText": "首页"
			}
		}
	    ,{
            "path" : "pages/list/list",
            "style" :                                                                                    
            {
                "navigationBarTitleText": "榜单",
                "enablePullDownRefresh": false
            }
            
        }
        ,{
            "path" : "pages/me/me",
            "style" :                                                                                    
            {
                "navigationBarTitleText": "个人首页",
                "enablePullDownRefresh": false
            }
            
        }
        ,{
            "path" : "pages/detail/detail",
            "style" :                                                                                    
            {
                "navigationStyle":"custom"
            }
            
        }
    ],
	"globalStyle": {
		"navigationBarTextStyle": "white",
		"navigationBarTitleText": "uni-app",
		"navigationBarBackgroundColor": "#42bd56",
		"backgroundColor": "#42bd56"
	},
	"tabBar": {
	    "color": "#7A7E83",
	    "selectedColor": "#42bd56",
	    "borderStyle": "black",
	    "backgroundColor": "#ffffff",
		// "fontSize":"30rpx",
	    "list": [{
	        "pagePath": "pages/index/index",
	        "iconPath": "static/tabs/shouye.png",
	        "selectedIconPath": "static/tabs/shouyeHL.png",
	        "text": "首页"
	    }, {
	        "pagePath": "pages/list/list",
	        "iconPath": "static/tabs/dingdan.png",
	        "selectedIconPath": "static/tabs/dingdanHL.png",
	        "text": "榜单"
	    }, {
			"pagePath": "pages/me/me",
			"iconPath": "static/tabs/wode.png",
			"selectedIconPath": "static/tabs/wodeHL.png",
			"text": "我的"
		}]
	}
}
