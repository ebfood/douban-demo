.list-category {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-pack: center;
  -webkit-justify-content: center;
          justify-content: center;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
  margin-bottom: 30rpx;
}
.list-category .cover {
  width: 250rpx;
  height: 250rpx;
  background-color: red;
  border-radius: 20rpx;
  display: inline-block;
  width: 250rpx;
  height: 250rpx;
  background: url(data:image/png;base64,) no-repeat center;
  background-size: contain;
}
.list-category .text {
  -webkit-box-flex: 1;
  -webkit-flex: 1;
          flex: 1;
  padding: 30rpx;
  font-size: 28rpx;
  border: 1px solid #c8c7cc;
  border-left: none;
  border-radius: 0 12rpx 12rpx 0;
}
.list-category .text .top {
  font-size: 36rpx;
  color: #666;
}
.list-category .text .top text {
  margin-left: 20rpx;
  color: #ffae33;
  font-size: 32rpx;
  line-height: 60rpx;
}

